- name: Create Tensorboard
  google.cloud.gcp_vertexai_tensorboard:
    state: present
    display_name: my-tensorboard
    description: sample description
    labels:
      key1: value1
      key2: value2
    region: us-central1
    project: "{{`{{ gcp_project }}`}}"
    auth_kind: "{{`{{ gcp_cred_kind }}`}}"
    service_account_file: "{{`{{ gcp_cred_file }}`}}"