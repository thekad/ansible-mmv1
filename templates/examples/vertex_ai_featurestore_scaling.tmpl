- name: Create Featurestore with scaling configuration
  google.cloud.gcp_vertexai_featurestore:
    state: present
    name: "{{`{{ resource_name }}`}}"
    region: us-central1
    online_serving_config:
      scaling:
        min_node_count: 1
        max_node_count: 10
    project: "{{`{{ gcp_project }}`}}"
    auth_kind: "{{`{{ gcp_cred_kind }}`}}"
    service_account_file: "{{`{{ gcp_cred_file }}`}}"