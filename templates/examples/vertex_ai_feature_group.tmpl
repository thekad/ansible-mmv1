- name: Create feature group
  google.cloud.gcp_vertexai_feature_group:
    state: present
    name: my_feature_group
    description: "My feature group"
    region: us-central1
    big_query:
      big_query_source:
        input_uri: "bq://{{`{{ gcp_project }}`}}.my_dataset.my_table"
    project: "{{`{{ gcp_project }}`}}"
    auth_kind: "{{`{{ gcp_cred_kind }}`}}"
    service_account_file: "{{`{{ gcp_cred_file }}`}}"