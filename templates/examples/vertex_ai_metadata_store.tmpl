- name: Create Metadata Store
  google.cloud.gcp_vertexai_metadata_store:
    state: present
    name: "{{`{{ resource_name }}`}}"
    region: us-central1
    project: "{{`{{ gcp_project }}`}}"
    auth_kind: "{{`{{ gcp_cred_kind }}`}}"
    service_account_file: "{{`{{ gcp_cred_file }}`}}"
  register: _mymds

- name: Delete Metadata Store
  google.cloud.gcp_vertexai_metadata_store:
    state: absent
    name: "{{`{{ resource_name }}`}}"
    region: us-central1
    project: "{{`{{ gcp_project }}`}}"
    auth_kind: "{{`{{ gcp_cred_kind }}`}}"
    service_account_file: "{{`{{ gcp_cred_file }}`}}"